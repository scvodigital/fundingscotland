<body class="mdc-theme--footer-dark-bg {{#if request.params.path}}{{replace request.params.path '_' ' '}}{{else}}home{{/if}}">
<!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-TJQ3FN"
  height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
  <fundingscotland-site>
    <div class="mdc-typography">
      {{#ifAny request.params.query.debug (contains request.url.hostname ".local") ~}}
        {{#if errors.[0]}}
          <div style="z-index:100;position:fixed;top:0;right:0;padding:5px;background-color:#ff0000;color:#ffffff;">
            Route Task Errors (F12)
          </div>
        {{/if}}
        {{#if logs.[0]}}
          <div style="z-index:100;position:fixed;bottom:0;right:0;padding:5px;background-color:#0000ff;color:#ffffff;"
               onclick="document.querySelector('#route-debug-log').style.display='block';">
            Route Log
          </div>
          <div style="z-index:100;position:fixed;bottom:0;left:0;right:0;padding:5px;background-color:#ededed;color:#000000;overflow:auto;height:50vh;display:none;border-top:1px solid #cccccc;" id="route-debug-log">
            <a style="position:fixed;right:25px;" onclick="document.querySelector('#route-debug-log').style.display='none';">x</a>
<pre style="white-space: pre-wrap;">
{{~#each logs}}
{{{this}}}

{{/each~}}
</pre>
          </div>
        {{/if}}
      {{/ifAny}}

      <header class="mdc-top-app-bar mdc-elevation--z5 top-bar-stuck" id="top-bar">
        <div class="container-fluid">
          <div class="mdc-top-app-bar__row mdc-top-app-bar__lower scvo-tab-bar hide-sm hide-md">
            <section class="mdc-top-app-bar__row mdc-top-app-bar__section--align-start">
              {{> components_scvo_bar}}
            </section>
          </div>
          <div class="mdc-top-app-bar__row mdc-top-app-bar__upper">
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start mdc-top-app-bar__section--logo">
              <a href="https://scvo.scot">
                <img src="{{~>asset_url url='images/logos/scvo-white.svg'~}}" alt="Scottish Council for Voluntary Organisations" />
                {{!-- <!-- <img src="{{~>asset_url url='images/logos/scvo-legacy-white.png'~}}" alt="Scottish Council for Voluntary Organisations" /> --> --}}
              </a>
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start mdc-top-app-bar__section--name">
              <a href="/" class="mdc-top-app-bar__title">
                {{@root.data.currentSite.title}}
                {{#ifAll @root.data.auth (compare @root.data.currentSite.name "===" "default") ~}}
                {{#>premium_content}}
                <span class="mdc-typography--caption">
                  Premium
                </span>
                {{/premium_content}}
                {{/ifAll ~}}
                {{#unless @root.data.currentSite.live ~}}
                <span class="mdc-typography--caption">
                  Test site
                </span>
                {{/unless ~}}
              </a>
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end">
              {{#ifAll @root.data.auth (compare @root.data.currentSite.name "===" "default") (contains request.url.hostname ".local") ~}}
              <div class="mdc-chipset pull-right hide-md hide-sm">
                <a href="/premium-toggle" class="mdc-chip mdc-chip--premium">
                  {{#>premium_content}}
                    <i class="far mdc-chip__icon fa-check-circle"></i>
                  {{/premium_content}}
                  {{#>premium_content_alt}}
                    <i class="far mdc-chip__icon fa-circle"></i>
                  {{/premium_content_alt}}
                  <div class="mdc-chip__text">
                    Premium features
                  </div>
                </a>
              </div>
              {{/ifAll ~}}

              {{#unless @root.route.metaData.hideSearch ~}}
              <button class="mdc-top-app-bar__navigation-icon hide-lg filter-search-button" id="sidebar-button" title="Search">
                <span class="far fa-fw fa-2x fa-search"></span>
              </button>
              {{/unless ~}}
              <button class="mdc-top-app-bar__navigation-icon hide-lg" data-drawer-target="#mobile-menu">
                <span class="far fa-fw fa-2x fa-bars"></span>
              </button>
            </section>
          {{!-- <!-- </div>
          <div class="mdc-top-app-bar__row mdc-top-app-bar__lower hide-sm hide-md mdc-theme--primary-dark-bg"> --> --}}
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end mdc-top-app-bar__main hide-md hide-sm">
              {{#> components_tab_bar
                   extraClasses="mdc-tab-bar__auto-width"
                   id="main-tab-bar"}}
                {{> components_tab
                    url="/"
                    labelClasses="mdc-theme--on-primary"
                    active=(regexMatch @root.request.url.pathname "^/$" "gi")
                    label="Home"}}
                {{> components_tab
                    url="/search"
                    labelClasses="mdc-theme--on-primary"
                    active=(regexMatch @root.request.url.pathname "^/search$|^/fund/" "gi")
                    label="Funds"}}
                {{> components_tab
                    url="/funders"
                    labelClasses="mdc-theme--on-primary"
                    active=(regexMatch @root.request.url.pathname "^/funders$|^/funder" "gi")
                    label="Funders"}}
                {{#each (filter @root.data.menus.hits.hits "_source.which_menus" "testIn" "main") ~}}
                  {{> components_tab
                      url=(default _source.menu_url (stripTrailingSlash (concat "/" (replace _source.slug (concat @root.data.currentSite.name "_") ""))))
                      labelClasses="mdc-theme--on-primary"
                      active=(regexMatch @root.request.url.pathname (concat "/" (replace _source.slug (concat @root.data.currentSite.name "_") "")) "gi")
                      label=(default _source.menu_label _source.title)}}
                {{/each ~}}
                {{> components_tab
                    url="/news"
                    labelClasses="mdc-theme--on-primary"
                    active=(regexMatch @root.request.url.pathname "^/news|^/post" "gi")
                    label="News"}}
                {{#compare @root.data.currentSite.name "===" "default" ~}}
                {{#unless @root.data.auth ~}}
                  {{#each @root.context.metaData.menus.user-not-signed-in ~}}
                    {{> components_tab
                        url=(stripTrailingSlash path)
                        labelClasses="mdc-theme--on-primary"
                        active=(regexMatch @root.request.url.pathname route "gi")
                        label=label}}
                  {{/each ~}}
                {{/unless ~}}
                {{/compare}}
              {{/components_tab_bar}}
              {{#compare @root.data.currentSite.name "===" "default" ~}}
              {{#if @root.data.auth ~}}
                {{#>components_anchored_menu
                    buttonClasses="mdc-theme--on-primary"
                    imageUrl=(default @root.data.auth.photoURL false)
                    imageClasses="far fa-fw user-avatar"
                    iconClasses=(if (if @root.data.auth.photoURL false else=true) "far fa-fw fa-user-circle" else=false)
                    buttonLabel="My account"
                    arrowClasses="far fa-fw fa-angle-down"}}
                    {{#if @root.data.profile.isAdmin ~}}
                      <a class="mdc-list-item" href="/admin" role="menuitem">
                        <span class="far fa-shield mdc-list-item__graphic" aria-hidden="true"></span>
                        Admin
                      </a>
                    {{/if ~}}
                  {{#each @root.context.metaData.menus.user-signed-in ~}}
                    <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}" role="menuitem">
                      {{#if icon ~}}
                      <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
                      {{/if ~}}
                      {{{label}}}
                    </a>
                  {{/each ~}}
                {{/components_anchored_menu}}
              {{/if ~}}
              {{/compare}}
            </section>
          </div>
        </div>
      </header>

      <div class="header-banner mdc-top-app-bar-fixed-adjust" style="background-image:url('{{@root.data.currentSite.headerImage}}')">
        {{#compare @root.data.currentSite.name "!==" "default" ~}}
        <div class="site-brand rounded mdc-elevation--z2">
          <a href="/" class="site-brand--organisation">
            <img src="{{@root.data.currentSite.logo}}" alt="{{@root.data.currentSite.organisationName}}"{{#unless @root.data.currentSite.live}} class="test-logo"{{/unless}} />
          </a>
          <a href="https://funding.scot" class="site-brand--fundingscotland">
            Funding <br />Scotland
          </a>
        </div>
        {{/compare ~}}
      </div>

      <aside class="mdc-drawer mdc-drawer--modal" id="mobile-menu" data-mdc-auto-init="MDCDrawer">
        <div class="mdc-drawer__content">
          <nav class="mdc-list">
            <a class="mdc-list-item {{#compare @root.request.url.pathname "/"}} mdc-list-item--activated{{/compare}}"
               href="/" aria-selected="true" tabindex="1">
              <span class="far fa-home mdc-list-item__graphic" aria-hidden="true"></span>
              Home
            </a>
            <a class="mdc-list-item {{#compare @root.request.url.pathname "/search"}} mdc-list-item--activated{{/compare}}"
               href="/search" aria-selected="true" tabindex="1">
              <span class="far fa-search mdc-list-item__graphic" aria-hidden="true"></span>
              Funds
            </a>
            <a class="mdc-list-item {{#compare @root.request.url.pathname "/funders"}} mdc-list-item--activated{{/compare}}"
               href="/funders" aria-selected="true" tabindex="1">
              <span class="far fa-search-plus mdc-list-item__graphic" aria-hidden="true"></span>
              Funders
            </a>
            {{#each (filter @root.data.menus.hits.hits "_source.which_menus" "testIn" "main") ~}}
              <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname (concat "/" _source.slug) "gi")}} mdc-list-item--activated{{/if}}" href="{{default _source.menu_url (stripTrailingSlash (concat "/" (replace _source.slug (concat @root.data.currentSite.name "_") "")))}}" {{#if (regexMatch @root.request.url.pathname (concat "/" _source.slug) "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
                {{#if _source.menu_icon ~}}
                <span class="{{_source.menu_icon}} mdc-list-item__graphic" aria-hidden="true"></span>
                {{/if ~}}
                {{{default _source.menu_label _source.title}}}
              </a>
            {{/each ~}}
            <a class="mdc-list-item {{#compare @root.request.url.pathname "/news"}} mdc-list-item--activated{{/compare}}"
               href="/news" aria-selected="true" tabindex="1">
              <span class="far fa-newspaper mdc-list-item__graphic" aria-hidden="true"></span>
              News
            </a>

            <div role="separator" class="mdc-list-divider"></div>

            {{#compare @root.data.currentSite.name "===" "default" ~}}
            {{#if @root.data.auth ~}}
            <a class="mdc-list-item mdc-list-item__user" href="#">
              <span class="mdc-list-item__graphic" aria-hidden="true">
                <img src="{{@root.data.auth.photoURL}}" />
              </span>
              <span class="mdc-list-item__text">My account</span>
            </a>
            {{#each @root.context.metaData.menus.user-signed-in ~}}
            {{#compare path "===" "/" ~}}
            {{else}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}"
               href="{{stripTrailingSlash path}}" {{#if (regexMatch @root.request.url.pathname route "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/compare ~}}
            {{/each ~}}
            {{else}}
            {{#each @root.context.metaData.menus.user-not-signed-in ~}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}"
               href="{{stripTrailingSlash path}}" {{#if (regexMatch @root.request.url.pathname route "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            {{/if ~}}
            {{/compare}}
          </nav>
        </div>
      </aside>

      <div class="mdc-drawer-scrim"></div>

      {{#compare @root.data.currentSite.name "!==" "default"}}
        <main class="mdc-theme--primary-light-bg main-body">
          {{> layout_migration}}
        </main>
      {{/compare}}
      <main class="mdc-theme--primary-light-bg main-body {{#if layoutParts.sidebar}}drawer-container-permanent{{else}}drawer-container-temporary{{/if}}">

        {{#if layoutParts.sidebar ~}}
        <aside class="mdc-drawer mdc-drawer--occasional" id="sidebar-temporary" data-menu-button=".filter-search-button">
          {{{layoutParts.sidebar}}}
        </aside>
        <div class="mdc-drawer-scrim"></div>
        {{/if ~}}

        {{{layoutParts.body}}}
      </main>
      {{{layoutParts.bodyLower}}}

      <footer class="mdc-layout__footer mdc-theme--footer-bg mdc-theme--on-footer">
        <div class="footer-main">
          <div class="container-fluid">
            <div class="mdc-layout-grid">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-4-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone contact">
                  <h3>Contact</h3>
                  <ul class="fa-ul">
                    <li>
                      <span class="fa-li"><span class="fas fa-envelope"></span></span>
                      {{#if @root.data.currentSite.live ~}}
                      <a href="mailto:{{@root.data.currentSite.organisationContact}}">{{@root.data.currentSite.organisationContact}}</a>
                      {{else}}
                      <a href="mailto:{{@root.context.metaData.site-configurations.default.organisationContact}}">{{@root.context.metaData.site-configurations.default.organisationContact}}</a>
                      {{/if ~}}
                    </li>
                    {{!-- <li>
                      <span class="fa-li"><span class="fas fa-phone fa-flip-horizontal"></span></span>
                      <a href="tel:+448001690022">0800 169 0022</a>
                    </li> --}}
                    <li>
                      <span class="fa-li"><span class="fab fa-twitter"></span></span>
                      <a href="https://twitter.com/fundingscotland" target="_blank">Twitter</a>
                    </li>
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone help">
                  <h3>Help</h3>
                  <ul class="mdc-list">
                    {{#each (filter @root.data.menus.hits.hits "_source.which_menus" "testIn" "footer-information") ~}}
                    <li class="mdc-list-item">
                      <a href="{{default _source.menu_url (concat "/" (replace _source.slug (concat @root.data.currentSite.name "_") ""))}}">{{{default _source.menu_label _source.title}}}</a>
                    </li>
                    {{/each ~}}
                    <li class="mdc-list-item">
                      <a href="/contact">Contact</a>
                    </li>
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone help">
                  <h3>Information</h3>
                  <ul class="mdc-list">
                    <li class="mdc-list-item">
                      <a href="/accessibility">Accessibility</a>
                    </li>
                    <li class="mdc-list-item">
                      <a href="/terms">Terms &amp; conditions</a>
                    </li>
                    <li class="mdc-list-item">
                      <a href="/privacy">Privacy</a>
                    </li>
                    <li class="mdc-list-item">
                      <a href="/cookies">Cookies</a>
                    </li>
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-4-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone about">
