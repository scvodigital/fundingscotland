{
  "name": "home_not_authenticated",
  "debug": false,
  "pattern": "/home-not-authenticated",
  "metaData": {
    "hideSearch": true
  },
  "tasks": [
    "context.metaData.commonTasks.menus",
    {
      "name": "primaryResponse",
      "taskModule": "elasticsearch",
      "config": {
        "connectionStringTemplate": "{$../data/elasticsearch-legacy-url.txt}",
        "elasticsearchConfig": {
          "apiVersion": "5.6"
        },
        "queryTemplates": {
          "index": "{$../data/elasticsearch-index.txt}",
          "type": "fs-fund",
          "template": ">context.metaData.handlebars.queries.funds_home-latest"
        }
      },
      "renderer": "handlebars"
    },
    {
      "name": "supplimentaryResponses",
      "taskModule": "elasticsearch",
      "config": {
        "connectionStringTemplate": "{$../data/elasticsearch-legacy-url.txt}",
        "elasticsearchConfig": {
          "apiVersion": "5.6"
        },
        "queryTemplates": [
          {
            "name": "types",
            "index": "{$../data/elasticsearch-index.txt}",
            "type": "fs-fund",
            "template": ">context.metaData.handlebars.queries.funds_types"
          },
          {
            "name": "splash",
            "index": "{$../data/elasticsearch-index.txt}",
            "type": "scvo-media",
            "template": ">context.metaData.handlebars.queries.pages_splash"
          }
        ]
      },
      "renderer": "handlebars"
    },
    {
      "name": "posts",
      "taskModule": "elasticsearch",
      "config": {
        "connectionStringTemplate": "{$../data/elasticsearch-url.txt}",
        "elasticsearchConfig": {
          "apiVersion": "5.6"
        },
        "queryTemplates": [
          {
            "name": "tfn_latest",
            "index": "",
            "type": "",
            "template": ">context.metaData.handlebars.queries.posts_tfn-latest"
          }
        ]
      },
      "renderer": "handlebars"
    },
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.pages_home-head-tags",
              "body": ">context.metaData.handlebars.templates.pages_home-not-authenticated",
              "foot": ">context.metaData.handlebars.templates.pages_home-foot"
            },
            "contentType": "text/html"
          }
        }
      }
    }
  ]
}